{
  "name": "@n-rowe/pash",
  "version": "0.1.0",
  "packageManager": "pnpm@10.2.1",
  "description": "A fast, minimal json patch generation library based off ohash's implementation",
  "author": {
    "name": "Nathan Rowe"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "unbuild",
    "test": "vitest",
    "test:ci": "vitest run",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache --fix ."
  },
  "dependencies": {
    "ohash": "^1.1.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.3.0",
    "@faker-js/faker": "^9.5.0",
    "eslint": "^9.20.1",
    "fast-json-patch": "^3.1.1",
    "typescript": "^5.7.3",
    "unbuild": "^3.3.1",
    "vitest": "^3.0.6"
  },
  "volta": {
    "node": "22.13.1",
    "pnpm": "10.2.1"
  }
}
